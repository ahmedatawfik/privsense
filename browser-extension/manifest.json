{
    "manifest_version": 3,
    "name": "PriVsense",
    "version": "1.0",
    "description": "A simple extension to showcase PriVsense",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_popup_width": 600,
      "default_popup_height": 400
    }
  }
  